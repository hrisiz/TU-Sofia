
public class Main {
	public static void main(String[] args) {
		Users users = new Users();
		users.LoadUsers();
		try {
			users.addUser(new User("user1123","goodpassword1"));
			users.addUser(new User("user2123","passowrd"));
			users.addUser(new User("user3123","hrisiz"));
			users.addUser(new User("user4123","1234567"));
		} catch (UserException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(users);
		users.SafeUsers();
	}
}
