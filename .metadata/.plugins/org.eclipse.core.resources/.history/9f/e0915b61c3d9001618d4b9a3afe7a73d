
public class User {
	private String username;
	private String password;
	public User(String user, String pass) {
		this.username = user;
		this.password = pass;
	}

	public boolean equals(User user) {
		if(user.username.equals(this.username) && user.password.equals(this.password)){
			return true;
		}
		return false;
	}
}
