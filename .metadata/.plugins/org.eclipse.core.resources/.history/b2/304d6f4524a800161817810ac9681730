
public class Main {
	public static void main(String[] args) {
		Users users = new Users();
		users.LoadUsers();
		try {
			users.addUser(new User("hrisiz","hahahaha"));
			users.addUser(new User("hrisiz","hahahaha"));
			users.addUser(new User("hrisiz","hahahaha"));
			users.addUser(new User("hrisiz","hahahaha"));
		} catch (UserException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
