
public class Main {
	public static void main(String[] args) {
		Users users = new Users();
		users.LoadUsers();
		try {
			users.addUser(new User("hrisiz","hahahaha"));
			users.addUser(new User("hrisiz1","123455677"));
			users.addUser(new User("hrisiz2","123468899"));
			users.addUser(new User("hrisiz3","976432345"));
		} catch (UserException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(users);
		users.SafeUsers();
	}
}
